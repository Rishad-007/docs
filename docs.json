{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "maple",
  "name": "Mithium",
  "colors": {
    "primary": "#ff0000",
    "dark": "#ff0000",
    "light": "#ff0000"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "global": {
      "anchors": [
        {
          "anchor": "Documentation",
          "icon": "book-open-cover",
          "href": "https://mintlify.com/docs"
        },
        {
          "anchor": "Blog",
          "icon": "newspaper",
          "href": "https://mintlify.com/blog"
        }
      ]
    },
    "tabs": [
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "General Information",
            "pages": ["index", "quickstart", "development", "webhooks", "help"]
          },
          {
            "group": "Customization",
            "pages": ["essentials/settings", "essentials/navigation"]
          },
          {
            "group": "Writing content",
            "pages": [
              "essentials/markdown",
              "essentials/code",
              "essentials/images",
              "essentials/reusable-snippets"
            ]
          },
          {
            "group": "AI tools",
            "pages": [
              "ai-tools/cursor",
              "ai-tools/claude-code",
              "ai-tools/windsurf"
            ]
          }
        ]
      },

      {
        "tab": "special section",
        "groups": [
          {
            "group": "Special group",
            "pages": ["special-section/Start-here"]
          },
          {
            "group": "Addresses",
            "pages": [
              "special-section/openapi.json get /addresses/{addressId}/balance",
              "special-section/openapi.json get /addresses/{addressId}/tokens/{tokenId}/balance",
              "special-section/openapi.json get /addresses/{addressId}/transactions",
              "special-section/openapi.json get /addresses/{addressId}/tokens/{tokenId}/transactions",
              "special-section/openapi.json get /addresses/{addressId}/tokens",
              "special-section/openapi.json post /addresses/{addressId}/transactions",
              "special-section/openapi.json post /addresses/{addressId}/tokens/{tokenId}/transfer",
              "special-section/openapi.json post /addresses/{addressId}/transactions/estimate",
              "special-section/openapi.json post /addresses/{addressId}/tokens/{tokenId}/estimate",
              "special-section/openapi.json post /addresses/{addressId}/transactions/sync",
              "special-section/openapi.json post /addresses/{addressId}/tokens/transactions/sync"
            ]
          },
          {
            "group": "Networks",
            "pages": ["special-section/openapi.json get /networks"]
          },
          {
            "group": "Safes",
            "pages": [
              "special-section/openapi.json post /safes",
              "special-section/openapi.json get /safes",
              "special-section/openapi.json post /safes/{safeId}/{blockchain}/{network}/generate",
              "special-section/openapi.json get /safes/{safeId}/addresses",
              "special-section/openapi.json get /safes/{safeId}/{blockchain}/{network}/addresses",
              "special-section/openapi.json get /safes/addresses/{addressId}",
              "special-section/openapi.json post /safes/{safeId}/export",
              "special-section/openapi.json post /safes/{safeId}/{blockchain}/{network}/{index}/private-key"
            ]
          },
          {
            "group": "Safe Permissions",
            "pages": [
              "special-section/openapi.json post /safes/{safeId}/permissions",
              "special-section/openapi.json get /safes/{safeId}/permissions",
              "special-section/openapi.json delete /safes/{safeId}/permissions/{userId}"
            ]
          },
          {
            "group": "Threshold Signatures",
            "pages": [
              "special-section/openapi.json post /safes/{safeId}/threshold/configure",
              "special-section/openapi.json get /safes/{safeId}/threshold/config",
              "special-section/openapi.json post /safes/{safeId}/signatures/sessions",
              "special-section/openapi.json get /safes/{safeId}/signatures/sessions",
              "special-section/openapi.json get /safes/signatures/sessions/{sessionId}",
              "special-section/openapi.json post /safes/signatures/sessions/{sessionId}/sign",
              "special-section/openapi.json post /safes/signatures/sessions/{sessionId}/reject"
            ]
          },
          {
            "group": "Tenant Transactions",
            "pages": [
              "special-section/openapi.json get /tenants/{tenantId}/transactions"
            ]
          },
          {
            "group": "App",
            "pages": ["special-section/openapi.json get /"]
          }
        ]
      }
    ]
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:hi@mintlify.com"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://dashboard.mintlify.com"
    }
  },
  "contextual": {
    "options": [
      "copy",
      "view",
      "chatgpt",
      "claude",
      "perplexity",
      "mcp",
      "cursor",
      "vscode"
    ]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/mintlify",
      "github": "https://github.com/mintlify",
      "linkedin": "https://linkedin.com/company/mintlify"
    }
  }
}
